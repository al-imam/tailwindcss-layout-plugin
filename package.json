{
  "name": "tailwind-layout",
  "version": "0.0.3",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "license": "MIT",
  "repository": "https://github.com/al-imam/tailwindcss-layout-plugin",
  "author": {
    "name": "al-imam",
    "email": "alimam01828@gmail.com"
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5"
  },
  "scripts": {
    "dev": "concurrently \"npm run serve\" \"npm run watch\"",
    "serve": "live-server . --no-browser",
    "watch": "npm run build -- -w",
    "build": "tailwindcss -o dist/tailwind.css"
  },
  "peerDependencies": {
    "tailwindcss": ">=3.0.0 || >= 3.0.0-alpha.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.6",
    "concurrently": "^5.3.0",
    "live-server": "^1.2.2",
    "postcss": "^8.4.13",
    "tailwindcss": "^3.0.24"
  }
}
